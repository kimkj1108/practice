package com.test01;

public class Hundai extends Car {
	String name; 
	@Override
	public void feeling() {
		System.out.println(this.name + "는 발을 이용해 조립합니다.");
		System.out.println(this.name + "디자인은 점점 산으로 갑니다.");
	}
	public void function(String brand_name) {//추상 클래스의 일반 메소드는 반드시 구현하지 않아도 된다.
		super.function(this.name);//아래 전역변수에 파라미터값을 너어주기 전인 이 코드의 this.name에는 아직 값이 들어가기 전이다.
		super.function(brand_name);
		this.name = brand_name;
		super.function(this.name);
		System.out.println(brand_name + "는 결함이 많습니다");
	}
	//부모 클래스의 전역변수는 자식클래스에 영향을 미치지 않는다.
	//부모 추상클래스의 일반 메소드를 재정의 하면 원래

}
