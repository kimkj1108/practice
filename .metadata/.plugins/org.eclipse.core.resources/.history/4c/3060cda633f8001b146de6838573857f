package com.test01;

public class Hundai extends Car {
	String name; 
	@Override
	public void feeling() {
		System.out.println(this.name + "는 발을 이용해 조립합니다.");
		System.out.println("디자인이 점점 산으로 갑니다.");
	}
	public void function(String brand_name) {//추상 클래스의 일반 메소드는 반드시 구현하지 않아도 된다.
		this.name = brand_name;
		System.out.println(brand_name + " 는 결함이 많습니다");
	}

}
